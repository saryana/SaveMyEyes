{
	"manifest_version": 2,
	
	"name": "Save My Eyes",
	"description": "Reminds you to look away from the screen to save eye strain.",
	"version": "0.0.1",
	
	"background": {
		"persistent": true,
		"scripts": ["thirdParty/jquery.min.js", "background.js"]
	},
	"permissions": [
		"storage"
	],

	"browser_action": {
		"desault_icon": "icon.png",
		"default_popup": "popup.html"
	}
}